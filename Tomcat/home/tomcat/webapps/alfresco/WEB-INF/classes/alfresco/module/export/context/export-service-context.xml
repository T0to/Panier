<beans>
    <bean id="exportService" class="org.alfresco.module.export.service.ExportServiceImpl" init-method="init">
    	<property name="moduleId" value="export"/>
    	<property name="nodeService" ref="NodeService"/>
    	<property name="serviceRegistry" ref="ServiceRegistry"/>
    	<property name="fileFolderService" ref="fileFolderService"/>
        
    </bean>
    
      <bean id="exportServiceScript" parent="baseJavaScriptExtension" class="org.alfresco.module.export.script.ExportServiceScript">
        <property name="extensionName">
            <value>exportService</value>
        </property>
        <property name="exportService" ref="exportService"/>
        <property name="serviceRegistry" ref="ServiceRegistry"/>
        <property name="fileFolderService" ref="fileFolderService"/>
        
    </bean>
    
